{
    "id": "ctaButtonLabel",
    "name": "Button Text",
    "group": "cta",
    "type": "text",
    "defaultValue": "SHOW ME",
    "condition": {
        "fieldID": "innerContentType",
        "operator": "match",
        "fieldValue": "CTA"
    }
},
{
    "id": "ctaButtonAction",
    "name": "Button Action",
    "group": "cta",
    "type": "select",
    "options": [
        {
            "label": "Redirect to link",
            "value": "redirect"
        },
        {
            "label": "Close pop-up",
            "value": "close"
        }
    ],
    "defaultValue": "redirect",
    "condition": {
        "fieldID": "innerContentType",
        "operator": "match",
        "fieldValue": "CTA"
    }
},
{
    "id": "ctaButtonLink",
    "name": "Button Target Link",
    "group": "cta",
    "type": "text",
    "defaultValue": "/your/target/page",
    "condition": {
        "fieldID": "innerContentType",
        "operator": "match",
        "fieldValue": "CTA"
    }
},